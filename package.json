{
  "name": "whal3sjs",
  "description": "Whal3s JavaScript Library",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Whal3s-xyz/whal3sJS.git"
  },
  "bugs": {
    "url": "https://github.com/Whal3s-xyz/whal3sJS/issues"
  },
  "homepage": "https://github.com/Whal3s-xyz/whal3sJS#readme",
  "keywords": [
    "web3"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Jonas Esser",
  "module": "dist/whal3s.esm.js",
  "size-limit": [
    {
      "path": "dist/whal3s.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/whal3s.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "husky": "^8.0.1",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "web3": "^1.7.5"
  }
}
